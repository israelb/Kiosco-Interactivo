package com.camaras{	import flash.display.DisplayObject;	public class duplicateMovieClip {		public function duplicateMovieClip(target:DisplayObject):DisplayObject {			// constructor code			var targetClass:Class;			targetClass = Object(target).constructor;			var duplicado:DisplayObject = new targetClass();			duplicado.transform = target.transform;			duplicado.filters = target.filters;			duplicado.cacheAsBitmap = target.cacheAsBitmap;			duplicado.opaqueBackground = target.opaqueBackground;			target.parent.addChild(duplicado);			return duplicado;			trace("duplicateMovieClip");		}	}}