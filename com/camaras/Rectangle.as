package com.camaras{	import flash.display.Sprite;	public class Rectangle extends Sprite {		private var xPos:Number;		private var yPos:Number;		private var rWidth:Number;		private var rHeight:Number;		private var color:uint;		public function Rectangle(xPos:Number=0,yPos:Number=0,rWidth:Number=10,rHeight:Number=10,color:uint=0x336699) {			this.graphics.beginFill(color);			this.graphics.drawRect(xPos,yPos,rWidth,rHeight);			this.graphics.endFill();		}	}}